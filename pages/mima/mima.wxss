.row {
    margin-top: 20rpx;
    display: flex;
    align-items: center;
    justify-content: space-around;
  }
  
  .row view {
    width: 80rpx;
    height: 80rpx;
    border-radius: 5rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    background-color:#ECECEC;
  }
  
  .row view input {
    width: 100%;
    height: 100%;
    text-align: center;
  }
  
  .active {
    border: 1px solid red !important;
  }
  
  .entry {
    width: 0;
    height: 0;
    opacity: 0;
  }
  